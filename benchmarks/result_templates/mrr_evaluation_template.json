      "avg_fix_iterations": 0.0,
      "avg_context_efficiency": 0.0,
      "avg_time_seconds": 0.0,
      "avg_tokens_used": 0
    },
    "category_breakdown": {
      "syntax_errors": {"success_rate": 0.0, "avg_iterations": 0.0},
      "logic_bugs": {"success_rate": 0.0, "avg_iterations": 0.0},
      "concurrency_issues": {"success_rate": 0.0, "avg_iterations": 0.0},
      "memory_problems": {"success_rate": 0.0, "avg_iterations": 0.0},
      "api_misuse": {"success_rate": 0.0, "avg_iterations": 0.0},
      "performance_bugs": {"success_rate": 0.0, "avg_iterations": 0.0}
    },
    "retrieval_performance": {
      "precision_curve": {
        "k_1": 0.0,
        "k_5": 0.0,
