{
  "dashboard": {
    "id": null,
    "title": "Kodezi Chronos Dashboard",
    "tags": ["chronos", "debugging", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Debug Success Rate",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "chronos_debug_success_rate * 100",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 67.3}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Average Iterations",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "chronos_debug_iterations_avg",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 15}
            ]
          }
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "chronos_cache_hit_rate * 100",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 70},
              {"color": "green", "value": 87}
            ]
          }
        }
      },
      {
        "id": 4,
        "title": "Total Sessions",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "chronos_total_sessions",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Debug Latency (p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, chronos_debug_duration_seconds)",
            "legendFormat": "p99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, chronos_debug_duration_seconds)",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, chronos_debug_duration_seconds)",
            "legendFormat": "p50",
            "refId": "C"
          }
        ]
      },
      {
        "id": 6,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(chronos_requests_total[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          },
          {
            "expr": "rate(chronos_errors_total[5m])",
            "legendFormat": "Errors/sec",
            "refId": "B"
          }
        ]
      },
      {
        "id": 7,
        "title": "Bug Types Distribution",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 16, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "chronos_bug_type_count",
            "legendFormat": "{{bug_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Model Confidence Distribution",
        "type": "heatmap",
        "gridPos": {"x": 8, "y": 16, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "chronos_model_confidence_histogram",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "graph",
        "gridPos": {"x": 16, "y": 16, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory %",
            "refId": "A"
          },
          {
            "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU %",
            "refId": "B"
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1
  }
}